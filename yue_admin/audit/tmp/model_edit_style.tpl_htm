//同步
$(function(){
	$("#send_same").click(function(){
		$(".table-list tbody td").find("font").each(function(i){
			if ($(this).html().length != 0 ) 
			{
				//window.alert('ok');
				$(this).prev("input").val($(this).html());
				$(this).parent("td").parent("tr").children("td").eq(0).find(".inputcheckbox").attr("checked","selected='true'");
			}
			/*else
			{
				window.alert("no");
			}*/
			
			//window.alert($(this).html());
		});
	});
})
