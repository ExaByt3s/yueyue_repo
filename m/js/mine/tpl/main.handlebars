            <a><!-- <i class="icon icon-my-edit-m"></i> --></a>
        </div>
    </header>

    <div class="content-body " data-role="container" style="height: 100%">
        <div data-role="scoll-wrapper" style="height: 100%;">
                <div data-role="refresh-bar-container" class="refresh-bar-container fn-hide" >

                </div>

                    <section class="group-user-info mt20  {{#if is_login}}{{else}}fn-hide{{/if}}" data-role="login-info">
                        <ul>


                            <!-- <li class="item head-icon-item fn-hide" data-role="nav-to-zone" data-role-name="cameraman">
                                <div data-role="zone" class="zone">
                                    <i class="icon icon-home-s"></i>
                                    <span>空间</span>
                                </div>
                                <div data-role="head-icon" class="head-img">
                                    <i class="img circle-img head-icon" style="background-image:url('{{user_icon}}');"></i>
                                </div>
                                <div class="name-and-location">
                                    <div class="user-name" data-role="user-name">{{nickname}}</div>
                                    <div class="location">
                                        <i class="icon icon-place"></i>
                                        <span data-role="location-name">{{city_name}}</span>
                                    </div>
                                </div>
